<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Chat" parent="android:Theme.Material.Light.NoActionBar" />
    <style name="RedButton" parent="ThemeOverlay.AppCompat">
        <item name="colorButtonNormal">@color/main</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="Icon">
        <item name="cornerSize">50%</item>
        <item name="android:padding">10dp</item>
        <item name="android:elevation">7dp</item>
    </style>
    <style name="FullImage">
        <item name="cornerSize">10%</item>
        <item name="android:padding">10dp</item>
        <item name="android:elevation">7dp</item>
        <item name="strokeWidth">10dp</item>
        <item name="strokeColor">@color/white</item>
    </style>
    <style name="MaterialButton">
        <item name="cornerSize">20%</item>
    </style>
    <style name="SeekBar"
        parent="Widget.AppCompat.SeekBar">
        <item name="colorAccent">@color/main</item>
    </style>
</resources>